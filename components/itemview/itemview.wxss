.item-navigator{
  width: 200rpx;
  margin-right: 20rpx;
  display: inline-block;
}
.item-navigator .item-group{
  width: 100%;
}
.item-group .thumbnail-group{
  width: 100%;
  height: 284rpx;
}
.thumbnail-group .thumbnail{
  height: 100%;
  width: 100%;
}
.item-group .item-title{
  font-size: 28rpx;
  text-align: center;
  margin-top: 10rpx;
  text-overflow: ellipsis;
  overflow: hidden;
}
 
.item-navigator:last-of-type{
  margin-right: 0;
}